{
  "node": true,
  "browser": true,
  "jquery": true,
  "devel": true,
  "strict": true,
  "evil": false,
  "indent": 2,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "eqnull": true,
  "forin": true,
  "immed": true,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "undef": true,
  "unused": "strict",
  "nonew": true,
  "quotmark": "single",
  "trailing": true,
  "maxparams": 3,
  "maxdepth": 3,
  "maxstatements": 15,
  "maxcomplexity": 5,
  "maxlen": 80,
  "freeze": true,
  "funcscope": true
}
